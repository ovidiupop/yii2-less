{
    "name": "ovidiupop/yii2-less.php",
    "description": "This is a Yii2 wrapper extension for PHP port of the official LESS processor http://lesscss.org.",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","less","less.php","php", "css","assets","assetManager","bundles","converter"],
    "license": "LGPL-V3",
    "authors": [
        {
            "name": "Ovidiu Pop",
            "email": "ovidiupop70@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "2.*",
	    "oyejorge/less.php": "1.7.0.*"
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0755"
                }
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "ovidiupop\\lessphp\\": ""
        }
    }
}
